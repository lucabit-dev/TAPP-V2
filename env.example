# Polygon.io API Configuration
POLYGON_API_KEY=your_polygon_api_key_here

# ChartsWatcher API Configuration
CHARTSWATCHER_USER_ID=your_user_id_here
CHARTSWATCHER_API_KEY=your_api_key_here
CHARTSWATCHER_CONFIG_ID=your_config_id_here

# Server Configuration
PORT=3001
NODE_ENV=production

# PnL WebSocket Configuration (set in Railway, not Vercel)
# NOTE: PNL_API_KEY is now server-side only for security
PNL_API_KEY=your_pnl_api_key_here
PNL_WS_BASE_URL=wss://sections-bot.inbitme.com

# Client-side Configuration (VITE_ prefix required for client-side access)
# NOTE: These variables should be set in Vercel, not in a local .env file
# They are shown here for reference only

# Backend Connection (set these to point to your Railway backend)
# VITE_API_BASE_URL=http://localhost:3001/api  # Local development
# VITE_API_BASE_URL=https://your-project.railway.app/api  # Production
# VITE_WS_BASE_URL=ws://localhost:3001  # Local development
# VITE_WS_BASE_URL=wss://your-project.railway.app  # Production
